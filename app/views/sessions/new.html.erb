


<h1> Login </h1>



<%= form_tag '/login' do %>
Username: <%= text_field_tag 'user_name'%> <br>
Password: <%= password_field_tag 'password'%> <br> <br> <br>
<%= submit_tag 'Log in' %>

<br>
<br>
<% end %>
<%= button_to "Create New Account", '/signup', method: "get" %>
<br>



<h3 style='color:red'>
<% if flash[:incorrect_login] %>
<%= flash[:incorrect_login] %>
<% end %>
</h3>
 